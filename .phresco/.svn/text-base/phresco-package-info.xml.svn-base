<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the Windows Phone project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
            <dependency>
                <groupId>com.photon.phresco.plugins</groupId>
                <artifactId>windows-phone-phresco-plugin</artifactId>
                <type>jar</type>
                <version>2.3.0.8000</version>
                <class>com.photon.phresco.plugins.windows.WPPlugin</class>
            </dependency>
        </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <value>2</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>Release</key>
                            <value>-Dconfiguration=Release</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>Debug</key>
                            <value>-Dconfiguration=Debug</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Configuration</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>configuration</key>
                    <possibleValues>
                        <value key="Release">Release</value>
                        <value key="Debug">Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>Release</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Platform</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>platform</key>
                    <possibleValues>
                        <value key="Any CPU">Any CPU</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>Any CPU</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">TYPE</value>
                    </name>
                    <type>hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>type</key>
                    <multiple>false</multiple>
                    <value>winphone</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <value>true</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
<groupId>com.photon.phresco.commons</groupId>
<artifactId>phresco-commons</artifactId>
<type>jar</type>
<version>3.0.0.10001-SNAPSHOT</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>true</multiple>
                    <value>Production</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>showErrors</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>
